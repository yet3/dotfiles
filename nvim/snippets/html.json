{
	"html doc": {
		"prefix": "doc",
		"body": [
			"<!DOCTYPE html>",
			"<html lang=\"en\">",
			"\t<head>",
			"\t\t<meta charset=\"UTF-8\" />",
			"\t\t<meta name=\"viewport\" content=\"width=device-width, initial-scale=1.0\" />",
			"\t\t<title>Title</title>",
			"\t</head>",
			"\t<body>",
			"\t\t$1",
			"\t</body>",
			"</html>"
		]
	},
	"add css stylesheet": {
		"prefix": "linkcss",
		"body": "<link rel=\"stylesheet\" href=\"${1}\" />"
	},
	"link": {
		"prefix": "link",
		"body": "<a href={$1} aria-label={ariaLabel} rel=\"noopener noreferrer\" target=\"_blank\">$2</a>"
	}
}
